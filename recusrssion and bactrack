
permutaiton of string:

class Solution {
    public ArrayList<String> findPermutation(String s) {
        // Code here
        HashSet<String>pq=new HashSet<>();
        String res="";
        permute(s,res,pq);
        ArrayList<String>ans=new ArrayList<>(pq);
        return ans;
        
        
    }
    public static void permute(String s,String res,HashSet<String>ans){
        if(s.isEmpty()){
            ans.add(res);
            return;
        }
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            String rem= s.substring(0,i)+s.substring(i+1);
            permute(rem,res+ch,ans);
        }
        
    }
}
